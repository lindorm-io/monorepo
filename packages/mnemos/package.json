{
  "name": "@lindorm/mnemos",
  "version": "0.3.1",
  "license": "AGPL-3.0-or-later",
  "author": "Jonn Nilsson",
  "repository": {
    "url": "https://github.com/lindorm-io/monorepo",
    "directory": "packages/mnemos"
  },
  "bugs": "https://github.com/lindorm-io/monorepo/issues",
  "publishConfig": {
    "access": "public"
  },
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "scripts": {
    "build": "rimraf dist && tsc -b ./tsconfig.build.json",
    "example": "ts-node example",
    "integration": "compd --file docker-compose.yml jest --config jest.config.integration.js --watch",
    "integration:focus": "compd --file docker-compose.yml jest --config jest.config.integration.js --watch $1",
    "prettier": "prettier --write ./src/*",
    "test": "jest --watch --",
    "test:ci": "npm run test:unit",
    "test:integration": "jest --config jest.config.integration.js --",
    "test:unit": "jest --config jest.config.js --",
    "typecheck": "tsc --watch",
    "typecheck:ci": "tsc",
    "update": "ncu -i",
    "update:auto": "ncu -u"
  },
  "dependencies": {
    "@lindorm/entity": "^0.2.1",
    "@lindorm/errors": "^0.1.8",
    "@lindorm/is": "^0.1.8",
    "@lindorm/scanner": "^0.3.3",
    "@lindorm/utils": "^0.5.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@lindorm/logger": "^0.3.0",
    "@lindorm/pylon": "^0.4.1",
    "@lindorm/types": "^0.3.0"
  }
}
