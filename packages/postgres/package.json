{
  "name": "@lindorm/postgres",
  "version": "0.3.7",
  "license": "AGPL-3.0-or-later",
  "author": "Jonn Nilsson",
  "repository": {
    "url": "https://github.com/lindorm-io/monorepo",
    "directory": "packages/postgres"
  },
  "bugs": "https://github.com/lindorm-io/monorepo/issues",
  "publishConfig": {
    "access": "public"
  },
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "scripts": {
    "build": "rimraf dist && tsc -b ./tsconfig.build.json",
    "example": "ts-node example",
    "integration": "compd --file docker-compose.yml jest --config jest.config.integration.mjs --watch",
    "integration:focus": "compd --file docker-compose.yml jest --config jest.config.integration.mjs --watch $1",
    "prettier": "prettier --write ./src/*",
    "test": "jest --watch --",
    "test:ci": "npm run test:unit",
    "test:integration": "jest --config jest.config.integration.mjs --",
    "test:unit": "jest --config jest.config.mjs --",
    "typecheck": "tsc --watch",
    "typecheck:ci": "tsc",
    "update": "ncu -i",
    "update:auto": "ncu -u"
  },
  "dependencies": {
    "@lindorm/errors": "^0.1.12",
    "@lindorm/is": "^0.1.11",
    "@lindorm/json-kit": "^0.5.3",
    "pg": "^8.13.0"
  },
  "devDependencies": {
    "@lindorm/logger": "^0.3.4",
    "@types/pg": "^8.11.10"
  }
}
