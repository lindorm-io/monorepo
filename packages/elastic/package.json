{
  "name": "@lindorm/elastic",
  "version": "0.0.0",
  "license": "AGPL-3.0-or-later",
  "author": "Jonn Nilsson",
  "repository": {
    "url": "https://github.com/lindorm-io/monorepo",
    "directory": "packages/elastic"
  },
  "bugs": "https://github.com/lindorm-io/monorepo/issues",
  "publishConfig": {
    "access": "public"
  },
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "scripts": {
    "build": "rimraf dist && tsc -b ./tsconfig.build.json",
    "example": "ts-node example",
    "integration": "compd --file docker-compose.yml jest --config jest.config.integration.js --watch",
    "integration:only": "compd --file docker-compose.yml jest --config jest.config.integration.js --watch $1",
    "test:ci": "npm run test:unit && npm run test:integration",
    "test:integration": "jest --config jest.config.integration.js --",
    "test:unit": "jest --config jest.config.js --",
    "test": "jest --watch --",
    "typecheck:ci": "tsc",
    "typecheck": "tsc --watch",
    "update": "ncu -u"
  },
  "dependencies": {
    "@elastic/elasticsearch": "^8.15.1",
    "@lindorm/case": "^0.1.6",
    "@lindorm/entity": "^0.1.0",
    "@lindorm/errors": "^0.1.6",
    "@lindorm/utils": "^0.4.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@lindorm/pylon": "^0.2.2",
    "@lindorm/types": "^0.2.0"
  }
}
